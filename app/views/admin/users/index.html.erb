
<h1>ユーザー一覧</h1>
<div class="row">
  <div class="col-md-8 offset-md-2">
    <ul class="list-group list-group-flush">
      <% @users.each do |user| %>
        <li class="list-group-item col">
          <%= user.name %> - <%=user.email %>
          <%= link_to '削除', admin_admin_dashboards_user_path(user), method: :delete, data: { confirm: '本当に削除しますか？' } %>
        </li>
      <% end %>
    </ul>
  </div>
</div>
<div class="row">
  <div class="col-md-8 offset-md-2">
    <%= link_to "back", admin_root_path, class: "btn btn-secondary mt-2" %>
  </div>
<!DOCTYPE html>
<html>
<head>
  <title>Admin - shift-management</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>

  <%= stylesheet_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>
<body>
  <% if admin_signed_in? %>
     <div class="text-right">
      <%= link_to 'ログアウト', destroy_admin_item_admin_session_path, method: :delete %>
    </div>

     <div class="text-right">
      <%= link_to ,"users", admin_dashboards_user_path, class:"btn btn-primary" %>
     </div>

     <div class="text-right">
       <%= link_to ,"groups", admin_dashboards_group_path, class:"btn btn-success" %>
     </div>

  <% end %>
  <%= yield %>
</body>
</html>